{
  "name": "@bolinjs/vite-plugin-log-label",
  "version": "1.0.0",
  "description": "A Vite plugin that replaces logs() with styled console.log outputs during compilation and provides type declarations",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "postinstall": "npx simple-git-hooks",
    "prepublishOnly": "npm run build"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "keywords": [
    "vite",
    "plugin",
    "label",
    "log",
    "console",
    "debug",
    "development"
  ],
  "author": {
    "name": "Bolin You",
    "email": "youbolin001@163.com"
  },
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/boli-duality/vite-plugin-log-label.git"
  },
  "bugs": {
    "url": "https://github.com/boli-duality/vite-plugin-log-label/issues"
  },
  "homepage": "https://github.com/boli-duality/vite-plugin-log-label#readme",
  "packageManager": "pnpm@10.6.2",
  "engines": {
    "node": "^22.0.0"
  },
  "devDependencies": {
    "@babel/generator": "^7.27.0",
    "@babel/parser": "^7.27.0",
    "@babel/traverse": "^7.27.0",
    "@babel/types": "^7.27.0",
    "@tsconfig/node22": "^22.0.1",
    "@types/babel__generator": "^7.27.0",
    "@types/babel__traverse": "^7.20.7",
    "@types/node": "^22.14.0",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.5.0",
    "eslint": "^9.24.0",
    "lint-staged": "^15.5.0",
    "local-pkg": "^1.1.1",
    "prettier": "^3.5.3",
    "simple-git-hooks": "^2.12.1",
    "unbuild": "^3.5.0",
    "vite": "^6.2.5"
  }
}
